<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="QAS Demo">
  <parameter name="driver.name" value="firefoxDriver"/>
  <test name="BDD Test">
    <method-selectors>
      <method-selector>
        <script language="beanshell">
          <![CDATA[testngMethod.getMethodName().equalsIgnoreCase("scenario2")||testngMethod.getMethodName().equalsIgnoreCase("sec")||testngMethod.getMethodName().equalsIgnoreCase("scenario1")||testngMethod.getMethodName().equalsIgnoreCase("scenario3")||testngMethod.getMethodName().equalsIgnoreCase("scenario7")]]>
        </script>
      </method-selector>
    </method-selectors>
    <classes>
      <class name="com.qmetry.qaf.automation.step.client.text.BDDTestFactory"/>
    </classes>
  </test> <!-- BDD Test -->
</suite> <!-- QAS Demo -->
